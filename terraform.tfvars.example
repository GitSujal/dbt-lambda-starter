# Example Terraform variables file
# Copy this to terraform.tfvars and update with your values

# AWS Region - choose your preferred region (e.g., us-east-1, eu-west-1, ap-southeast-2)
aws_region = "ap-southeast-2"

# Unique prefix for S3 buckets (must be 4-36 characters, lowercase)
# This should be unique across your AWS account
bucket_prefix = "my-company-dbt"

# Environment name: dev or prod
environment = "dev"

# Additional tags for resource identification and cost tracking
extra_tags = {
  Environment = "Development"
  Owner       = "data-team"
  CostCenter  = "engineering"
}
